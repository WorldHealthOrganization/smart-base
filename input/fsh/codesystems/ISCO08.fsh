* #1221 "Sales and Marketing Managers"
  * ^property[+].code = #definition
  * ^property[=].valueString = "Sales and marketing managers plan, direct and coordinate sales and marketing activities."
* #1222 "Advertising and Public Relations Managers"
  * ^property[+].code = #definition
  * ^property[=].valueString = "Advertising and public relations managers plan, direct and coordinate advertising and public relations activities."
* #1223 "Research and Development Managers"
  * ^property[+].code = #definition
  * ^property[=].valueString = "Research and development managers plan, direct and coordinate research and development activities of an enterprise or organization."
* #2111 "Physicists and Astronomers"
  * ^property[+].code = #definition
  * ^property[=].valueString = "Physicists and astronomers conduct research and improve or develop concepts, theories and operational methods in physics and astronomy."
* #2112 "Meteorologists"
  * ^property[+].code = #definition
  * ^property[=].valueString = "Meteorologists study and forecast weather and atmospheric conditions."
* #2113 "Chemists"
  * ^property[+].code = #definition
  * ^property[=].valueString = "Chemists conduct research and improve or develop concepts, theories and operational methods in chemistry."
* #2114 "Geologists and Geophysicists"
  * ^property[+].code = #definition
  * ^property[=].valueString = "Geologists and geophysicists conduct research on the composition, structure and other physical aspects of the Earth."
* #2120 "Mathematicians, Actuaries and Statisticians"
  * ^property[+].code = #definition
  * ^property[=].valueString = "Mathematicians, actuaries and statisticians conduct research and improve or develop mathematical, actuarial and statistical theories, concepts and methods."
* #2131 "Biologists, Botanists, Zoologists and Related Professionals"
  * ^property[+].code = #definition
  * ^property[=].valueString = "Biologists, botanists, zoologists and related professionals study living organisms and their interactions with each other and with the environment."
* #2132 "Farming, Forestry and Fisheries Advisers"
  * ^property[+].code = #definition
